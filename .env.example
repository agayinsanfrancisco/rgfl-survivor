# Postgres connection string for Prisma & your app
DATABASE_URL=postgresql://user:password@host:5432/database?sslmode=require

# JWT secret for authentication (set your own strong secret here!)
JWT_SECRET=your-secret-key-here

# Allowed front-end origins (comma separated)
CLIENT_ORIGIN=http://localhost:5000,https://your-app.onrender.com

# Optional: override server port locally
PORT=5050

# Keep setup endpoint disabled in environments you care about
ENABLE_SETUP_ENDPOINT=false
