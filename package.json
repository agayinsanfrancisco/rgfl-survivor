{
    "name": "rgfl-survivor",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "npm-run-all --parallel dev:server dev:client",
        "dev:server": "PORT=5050 NODE_ENV=development tsx server/index.ts",
        "dev:client": "vite --config vite.config.ts --host 0.0.0.0 --port 5000",
        "build:client": "vite build --config vite.config.ts",
        "build:server": "tsc --project tsconfig.json",
        "build": "npm run build:client && npm run build:server",
        "start": "NODE_ENV=production node dist/server/index.js",
        "postinstall": "prisma generate",
        "db:migrate": "prisma migrate deploy",
        "db:push": "prisma db push",
        "db:generate": "prisma generate",
        "db:seed": "tsx prisma/seed.ts",
        "check": "tsc",
        "test": "vitest run"
    },
    "dependencies": {
        "@prisma/client": "^6.16.2",
        "@types/compression": "^1.8.1",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.2",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/luxon": "^3.7.1",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.19.0",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.7.0",
        "axios": "^1.12.2",
        "bcryptjs": "^3.0.2",
        "compression": "^1.8.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.21.2",
        "express-rate-limit": "^7.4.0",
        "express-session": "^1.18.2",
        "jsonwebtoken": "^9.0.2",
        "luxon": "^3.7.0",
        "multer": "^2.0.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.2",
        "recharts": "^2.15.2",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "typescript": "5.6.3",
        "uuid": "^9.0.1",
        "vite": "^7.1.7",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@vitest/ui": "^3.2.4",
        "c8": "^10.1.3",
        "npm-run-all": "^4.1.5",
        "prisma": "^6.16.2",
        "tsx": "^4.20.5",
        "vitest": "^3.2.4"
    }
}
